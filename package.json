{
  "name": "kosenvj-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:main": "tsc src/main.ts --outDir dist/",
    "electron:main:watch": "tsc src/main.ts --outDir dist/ --watch",
    "electron:preload": "tsc src/preload.ts --outDir dist/",
    "electron:preload:watch": "tsc src/preload.ts --outDir dist/ --watch",
    "electron": "electron --enable-logging=log.txt dist/main.js",
    "dev:electron": "npm-run-all electron:main electron:preload --parallel dev electron"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@inlet/react-pixi": "^6.6.5",
    "@react-three/drei": "^8.3.1",
    "@react-three/fiber": "^7.0.24",
    "@unicode/unicode-14.0.0": "^1.2.1",
    "framer-motion": "4",
    "js-easing-functions": "^1.0.3",
    "mabiki": "^1.0.1",
    "pixi.js": "^6.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gamepads": "^1.0.0",
    "react-p5": "^1.3.22",
    "react-vfx": "^0.5.0",
    "react-youtube": "^7.14.0",
    "recoil": "^0.5.2",
    "three": "^0.135.0",
    "webmidi": "^3.0.6",
    "wouter": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@types/p5": "^1.3.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/three": "^0.135.0",
    "@vitejs/plugin-react": "^1.0.7",
    "electron": ">= 16.0.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
